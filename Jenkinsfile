pipeline {
    agent any
    
     stages{
     
      stage('Hello World'){
       steps {
         echo "Hello World"
       }
    }
    stage('Development'){
       steps {
         echo "Development"
           sleep 4
       }
    }
    stage('Testing'){
       steps {
         echo "Testing"
           sleep 4
       }
    }
    stage('QA Testing'){
       steps {
         echo "QA Testing"
           sleep 4
       }
    }
    stage('Deploy'){
       steps {
         echo "Deploy"
           sleep 4
       }
    }
    stage('Production'){
       steps {
         echo "Production"
           sleep 4
       }
    }
    
    }

}
